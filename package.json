{
  "name": "edenfirstproject",
  "version": "0.0.1",
  "private": true,
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "main": "src/module.js",
  "scripts": {
    "postinstall": "one-app-engine --setup",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start-e2e": "RN_SRC_EXT=e2e.js react-native start",
    "detox-build-android": "detox build --configuration android",
    "detux-test-android": "detox test --configuration android --watch",
    "start": "one-app-engine --mock-mode quickLogin -ia --force-localhost --app admin",
    "start-ios": "one-app-engine -i —force-localhost",
    "start-android": "one-app-engine -a —force-localhost",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start-admin": "one-app-engine --mock-mode unmocked -ia --force-localhost --app admin"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@reduxjs/toolkit": "1.6.1",
    "axios": "0.21.1",
    "react-redux": "7.2.4",
    "redux": "4.1.1"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "2.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.4.1",
    "@types/react-native": "^0.64.5",
    "@types/react-redux": "7.1.18",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "3.6.0",
    "eslint": "^7.14.0",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "jest-cli": "26.4.2",
    "jest-summary-reporter": "^0.0.2",
    "jest-tc-reporter": "^1.0.0",
    "react-native-codegen": "0.0.7",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^24.1.0",
    "@wix/native-app-wix-login": "ga",
    "@wix/wix-one-app-auth": "ga",
    "@wix/wix-one-app-member-view": "ga",
    "@wix/wix-one-app-platform": "ga",
    "@wix/wix-react-native-ui-lib": "ga",
    "@wix/wix-react-native-i18n": "ga",
    "@wix/wix-owners-app-platform": "ga",
    "@wix/wix-one-developer-utils": "ga",
    "@wix/react-native-wix-media": "ga",
    "@wix/wix-one-app-notifications": "ga",
    "wix-one-app-engine": "ga"
  },
  "peerDependencies": {
    "wix-one-app-engine": "*"
  },
  "resolutions": {
    "@types/react": "17"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "module:metro-react-native-babel-preset"
        ],
        "retainLines": true
      }
    }
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-vector-icons)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "oneAppEngine": {
    "modules": [
      "edenfirstproject",
      "demo-tab-module",
      "@wix/wix-one-app-platform",
      "@wix/wix-one-app-member-view",
      "@wix/native-app-wix-login",
      "@wix/wix-one-app-auth"
    ],
    "tabs": [
      "demoTab"
    ],
    "admin": {
      "modules": [
        "edenfirstproject",
        "demo-tab-module",
        "@wix/wix-owners-app-platform",
        "@wix/wix-one-app-platform",
        "@wix/wix-one-app-member-view",
        "@wix/wix-one-app-notifications",
        "@wix/wix-one-developer-utils",
        "@wix/native-app-wix-login"
      ],
      "tabs": [
        "homeTab",
        "wishlistTab"
      ]
    }
  }
}
